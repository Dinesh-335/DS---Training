#include <stdio.h>
#include <string.h>
int main() {
    char str[] = "MSI";
    int len = strlen(str);
    int isPalindrome = 1; 
    for (int i = 0; i < len / 2; i++) {
        if (str[i] != str[len - 1 - i]) {
            isPalindrome = 0;
            break;
        }
    }
    if (isPalindrome)
        printf("'%s' is a palindrome.\n", str);
    else
        printf("'%s' is NOT a palindrome.\n", str);
    return 0;
}
